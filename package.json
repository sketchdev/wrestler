{
  "name": "wrestler",
  "version": "0.9.0",
  "description": "An express middleware which provides a solid foundation for a RESTful API.",
  "keywords": [
    "restful",
    "api",
    "node",
    "express"
  ],
  "main": "wrestler.js",
  "scripts": {
    "test": "yarn test-nedb && yarn test-mongo && yarn test-pg",
    "test-nedb": "NODE_ENV=test nyc mocha --recursive --exit --slow 150",
    "test-mongo": "NODE_ENV=test MONGO_DB_URI=mongodb://localhost:27017/wrestler MONGO_DB_NAME=wrestler nyc mocha --recursive --exit --slow 150",
    "test-pg": "NODE_ENV=test PG_CONNECTION_STRING=postgresql://localhost:5432/wrestler nyc mocha --recursive --exit --slow 150",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start-mongo": "docker run --name wrestler -p 27017:27017 -d --rm mongo:latest",
    "stop-mongo": "docker container stop wrestler",
    "pg-db-migrate": "DATABASE_URL=postgresql://localhost:5432/wrestler db-migrate"
  },
  "author": "Cody Frederick",
  "bugs": {
    "url": "https://github.com/sketchdev/wrestler/issues"
  },
  "repository": {
    "url": "https://github.com/sketchdev/wrestler"
  },
  "license": "MIT",
  "dependencies": {
    "express-validator": "^6.9.2",
    "jsonwebtoken": "8.5.1",
    "knex": "^0.21.17",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongodb": "^3.1.1",
    "nedb": "^1.8.0",
    "nodemailer": "^6.4.17",
    "pg": "8.5.1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "3.1.0",
    "db-migrate": "0.11.12",
    "db-migrate-pg": "1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "mocha": "^8.2.1",
    "morgan": "^1.9.0",
    "nyc": "^15.1.0",
    "sinon": "9.2.4",
    "supertest": "6.1.3"
  }
}
